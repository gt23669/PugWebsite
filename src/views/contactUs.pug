

html 
    head
        title=title
        script(src="")
        link(rel="stylesheet", href="/css/stylesContact.css")
        link(rel="stylesheet", href="/css/stylesMenu.css")
    body
        div(class=["myContainer","roundedCorner"])
            h1(id="myH1")=h1Text
        div(class="centerContainer")
            ul(class="categories")
                li(class="homeT"): a(href="/") Home   
                li(class="contactT"): a(href="/contactUs") Contact Us  
                li(class="rosterT"): a(href="/roster") Roster   
                li(class="recordsT"): a(href="/records") Previous Season    
                li(class="ordersT"): a(href="/orders") Store
        div(class="contactData")
            each contact in contactData
                div(class="contacts")
                    div(class="contactname")= contact.name
                    div(class="contactphone")= contact.phone
                    div(class="contactemail")= contact.email
        


//- each contact in contactData
                        li = contact.name+" - "+contact.phone

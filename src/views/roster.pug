html 
    head
        title=title
        script(src="")
        link(rel="stylesheet", href="/css/stylesRoster.css")
        link(rel="stylesheet", href="/css/stylesMenu.css")
    body
        div(class=["myContainer","roundedCorner"])
            h1(id="myH1")=h1Text
        div(class="centerContainer")
            ul(class="categories")
                 li(class="homeT"): a(href="/") Home   
                    li(class="contactT"): a(href="/contactUs") Contact Us  
                    li(class="rosterT"): a(href="/roster") Roster   
                    li(class="recordsT"): a(href="/records") Previous Season    
                    li(class="ordersT"): a(href="/orders") Store
        div(class="teamRoster")
            table(class="teamTable")
                each member in members
                    div(class="teamMember")
                        tr
                            td(class="tdimage")  
                                img(src=member.image class="tableteamimage")                              
                            td(class="tablemembername")=member.name
                            td(class="tablemembernumber")=member.number
                            td(class="tablememberposition")=member.position
                            td(class="tableskill")=member.skill

